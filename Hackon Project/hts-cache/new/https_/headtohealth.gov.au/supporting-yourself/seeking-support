            <button type="button" class="content-card-show-more-button" data-bind="visible: !ShowMoreVisible() && Cards().length > $root.NumCardsToShow(), click: $root.ShowLess.bind($data, Name(), $element), attr: { 'aria-label': 'Show less ' + Title() }">Show Less</button>
            <button type="button" class="content-card-show-all-button" data-bind="text:'Show all ('+TotalCards() +')', visible: ShowMoreVisible(), click: $root.ShowMore.bind($data, Name(), true), attr: { 'aria-label': 'Show all ' + Title() }">Show all</button>
        </div>
        <div class="content-card-show-more-button-container list-view" data-bind="visible: !$root.IsInGridView()">
            <button type="button" class="content-card-show-more-button" data-bind="visible: !ShowMoreVisible() && Cards().length > $root.NumCardsToShow(), click: $root.ShowLess.bind($data, Name(), $element), attr: { 'aria-label': 'Show less ' + Title() }">Show Less</button>
            <button type="button" class="content-card-show-all-button" data-bind="text:'Show all ('+TotalCards() +')', visible: ShowMoreVisible(), click: $root.ShowMore.bind($data, Name(), true), attr: { 'aria-label': 'Show all ' + Title() }">Show all</button>
        </div>
        <!-- /ko -->
    </div>
